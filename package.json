{
  "name": "@v-bible/liturgical-calendar-generator",
  "version": "1.1.1",
  "author": "v-bible",
  "description": "A library to generate Catholic liturgical calendars",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v-bible/liturgical-calendar-generator.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "liturgical"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "liturgical-calendar-generator": "./dist/cli.js",
    "__liturgical-calendar-generator_bash_complete": "./dist/bash-complete.js"
  },
  "scripts": {
    "prepare": "husky",
    "lint": "tsc",
    "build": "tsup",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "prepublish": "pnpm run build && pnpm run check-exports && pnpm run lint",
    "postinstall": "npx @stricli/auto-complete@latest install liturgical-calendar-generator --bash __liturgical-calendar-generator_bash_complete"
  },
  "dependencies": {
    "@stricli/auto-complete": "1.2.4",
    "@stricli/core": "1.2.4",
    "@v-bible/js-sdk": "1.0.16",
    "date-fns": "4.1.0",
    "dotenv": "17.2.3",
    "es-toolkit": "1.43.0",
    "i18next": "25.7.3",
    "i18next-chained-backend": "4.6.3",
    "i18next-fs-backend": "2.6.1",
    "i18next-intervalplural-postprocessor": "3.0.0",
    "ical-generator": "10.0.0",
    "intl-pluralrules": "2.0.1",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@babel/core": "7.28.5",
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@types/node": "24.10.4",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.51.0",
    "@vitest/eslint-plugin": "1.6.4",
    "@vitest/ui": "4.0.16",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
